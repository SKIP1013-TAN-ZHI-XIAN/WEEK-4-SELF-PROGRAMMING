package selfProgramming;

class Expressions {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println(1 + (4 * 3));
		System.out.println("3 * 4 = " + (3 * 4));  // To print 3 * 4 on screen
		System.out.println((1 + 2) + 3);
		System.out.println((1 - 2) - 3);
		System.out.println(1 - (2 - 3));
	}

}
